	<queryString>
		<![CDATA[SELECT
     escolas.`id` AS escolas_e_id,
     escolas.`siem_id` AS escolas_e_siems_id,
     escolas.distrito AS escolas_e_distrito,
     escolas.`bairro` AS escolas_e_bairro,
     escolas.`pessoa_id` AS escolas_e_funcionario,
     escolas.`pregao1` AS escolas_e_pregao1,
     escolas.`pregao2` AS escolas_e_pregao2,
     escolas.`pregao3` AS escolas_e_pregao3,
     escolas.`pregao4` AS escolas_e_pregao4,
     escolas.`possui_internet_lab` AS escolas_e_possui_internet_lab,
     escolas.`qt_monitores_lab` AS escolas_e_qt_monitores_lab,
     siems.`id` AS siems_s_id,
     siems.`siem` AS siems_s_siems,
     siems.`nome` AS siems_s_nome,
     siems.`tipo_escola` AS siems_s_tipo_escolas,
     escolas.`possui_analista` AS escolas_e_possui_analista,
     escolas.`possui_lab` AS escolas_e_possui_lab,
     pessoas.`id` AS pessoas_id,
     pessoas.`nome` AS pessoas_nome
FROM
     `siems` siems INNER JOIN `escolas` escolas ON siems.`id` = escolas.`siem_id`
     INNER JOIN `pessoas` pessoas ON escolas.`pessoa_id` = pessoas.`id`
WHERE
     possui_lab = "SIM"
 AND escolas.distrito = $P{distrito}
 AND possui_analista = $P{articulador}
 AND possui_internet_lab = $P{internet}]]>
	</queryString>
